#include <stdio.h>

int main(){

    int a = 42;
    int b = 3;
    int c = -7;
    float d = -6.99999; // da se doda jos dve 9 rezultat bi bio drugaciji jer zaokruzuje posle toliko cifara na float i zaokruzi se na - 7
    unsigned char e;

    e = a <= b || c != d && c < b;

    // 42 <= 3 || -7 != -6.9999 && -3 < 3;
    // 0 || 1 && 1
    // 0 || 1
    // 1

    printf ("Rezultat dela a <= b je %u. \n", a <= b);
    printf ("Rezultat dela c != d je %u. \n", c != d);
    printf ("Rezultat dela c < b je %u. \n", c < b);

    printf ("Konacni rezultat je %u. \n", e);


}
